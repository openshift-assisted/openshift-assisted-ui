{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "applications/ui-integration-e2e/src",
  "projectType": "application",
  "targets": {
    "run-tests": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "applications/ui-integration-e2e/cypress.json",
        "devServerTarget": "ui-integration:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "ui-integration:serve:production"
        }
      }
    },
    "open-tests": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "yarn cypress open --project=applications/ui-integration-e2e"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "applications/ui-integration-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "ui-integration"
  ]
}